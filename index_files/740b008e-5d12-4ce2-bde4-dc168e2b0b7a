document.body.appendChild(document.createElement('snappylabs-chat')).setAttribute("bot-id","740b008e-5d12-4ce2-bde4-dc168e2b0b7a");function injectScript(src){return new Promise((resolve,reject)=>{const script=document.createElement('script');script.async=true;script.src=src;script.addEventListener('load',resolve);script.addEventListener('error',()=>reject('Error loading script.'));script.addEventListener('abort',()=>reject('Script loading aborted.'));document.head.appendChild(script);});}
injectScript("https://snappychat.snappylabs.io/snappylabs-chat.min.js");